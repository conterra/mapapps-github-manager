{
  "appName": "management",
  "properties": {
    "title": "Manager"
  },
  "load": {
    "bundleLocations": [
        "localbundles",
        "bundles"
    ],
    "allowedBundles": [
      "system",
      "splashscreen",
      "notifier",
      "console",
      "templatelayout",
      "toolset",
      "managementlayout",
      "theme-everlasting",
      "mapapps-github-manager"
    ]
  },
  "bundles": {
    "toolset": {
      "ToolsetManager": {
        "toolsets": [
          {
            "id": "default_tools",
            "tools": [
              "appmanagement_*"
            ],
            "container": "desktopbackground",
            "cssClass": "buildertoolsMenuBar",
            "windowType": "menubar",
            "position": {
              "rel_t": 7,
              "rel_l": 10
            }
          }
        ]
      }
    }
  }
}
